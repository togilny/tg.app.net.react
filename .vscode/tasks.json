{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-server",
      "type": "shell",
      "command": "dotnet build",
      "options": {
        "cwd": "${workspaceFolder}/server"
      },
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build-client",
      "type": "shell",
      "command": "npm run build",
      "options": {
        "cwd": "${workspaceFolder}/client"
      },
      "problemMatcher": []
    },
    {
      "label": "npm-install",
      "type": "shell",
      "command": "npm install",
      "options": {
        "cwd": "${workspaceFolder}/client"
      },
      "isBackground": false,
      "problemMatcher": []
    }
  ]
}
